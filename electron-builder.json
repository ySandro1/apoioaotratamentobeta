{
  "appId": "com.rdSaude.apoio-tratamento",
  "productName": "RD Saúde - Apoio ao Tratamento",
  "directories": {
    "output": "dist-electron"
  },
  "files": [
    "dist/**/*",
    "electron.js",
    "package.json"
  ],
  "extraResources": [
    {
      "from": "public/pwa-icon-192.png",
      "to": "icon.png"
    }
  ],
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": ["x64"]
      },
      {
        "target": "portable",
        "arch": ["x64"]
      }
    ],
    "icon": "public/pwa-icon-192.png",
    "publisherName": "Alexsandro - Itumbiara 3",
    "requestedExecutionLevel": "asInvoker"
  },
  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true,
    "createDesktopShortcut": true,
    "createStartMenuShortcut": true,
    "shortcutName": "RD Saúde",
    "installerIcon": "public/pwa-icon-192.png",
    "uninstallerIcon": "public/pwa-icon-192.png"
  },
  "portable": {
    "artifactName": "RD-Saude-Portable-${version}.exe"
  },
  "mac": {
    "target": "dmg",
    "icon": "public/pwa-icon-192.png",
    "category": "public.app-category.medical"
  },
  "linux": {
    "target": "AppImage",
    "icon": "public/pwa-icon-192.png",
    "category": "Office"
  }
}